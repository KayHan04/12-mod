per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = int(input("Вводите сумму"))

TKB = int((per_cent['ТКБ'])*(money*0.1))
SKB = int((per_cent['СКБ'])*(money*0.1))
VTB = int((per_cent['ВТБ'])*(money*0.1))
SBER = int((per_cent['СБЕР'])*(money*0.1))

deposit = [TKB, SKB, VTB, SBER]
print("Накопленные средства", deposit)
